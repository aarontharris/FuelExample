<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/shared_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.common.fragment.exampleshared.SharedFragment">

    <TextView
        android:id="@+id/shared_fragment_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SharedFragment"
        app:layout_constraintBottom_toTopOf="@id/shared_fragment_custom_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.fuelexample.ui.common.view.SharedCustomView
        android:id="@+id/shared_fragment_custom_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shared_fragment_message" />

    <com.example.fuelexample.ui.common.view.SharedCustomView2
        android:id="@+id/shared_fragment_custom_view2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shared_fragment_message" />

</androidx.constraintlayout.widget.ConstraintLayout>
